{
  "name": "trademate",
  "version": "0.1.0",
  "description": "TradeMate multi cryptocurrency trading bot",
  "keywords": [
    "trademate",
    "bot",
    "trading",
    "cryptocurrency",
    "crypto",
    "bitcoin"
  ],
  "homepage": "./",
  "bugs": {
    "url": "https://github.com/woutheijnen/TradeMate/issues",
    "email": "woutheijnen@hotmail.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "Wout Heijnen",
    "email": "woutheijnen@hotmail.com",
    "url": "https://github.com/woutheijnen"
  },
  "contributors": [
    {
      "name": "Wout Heijnen",
      "email": "woutheijnen@hotmail.com",
      "url": "https://github.com/woutheijnen"
    }
  ],
  "main": "public/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/woutheijnen/TradeMate.git"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"sleep 6 && electron .\"",
    "build": "react-scripts build && electron-builder",
    "deploy": "git fetch --all && git reset --hard origin/master && npm install && npm run build",
    "test": "react-scripts test",
    "clear-modules": "rm -r -f ./node_modules",
    "refresh": "npm run clear-modules && npm run update && npm install",
    "update": "ncu -u -a"
  },
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.0.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "react-spinners": "^0.4.7",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.36.2",
    "electron-devtools-installer": "^2.2.4"
  },
  "build": {
    "appId": "com.woutheijnen.trademate",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "productName": "TradeMate",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
}
