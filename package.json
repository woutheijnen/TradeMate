{
  "name": "trademate",
  "version": "0.1.0",
  "description": "TradeMate multi cryptocurrency trading bot",
  "homepage": "./",
  "bugs": {
    "url": "https://github.com/woutheijnen/TradeMate/issues",
    "email": "woutheijnen@hotmail.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "Wout Heijnen",
    "email": "woutheijnen@hotmail.com",
    "url": "https://github.com/woutheijnen"
  },
  "main": "public/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/woutheijnen/TradeMate.git"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"sleep 6 && electron .\"",
    "build": "react-scripts build && electron-builder",
    "deploy": "git fetch --all && git reset --hard origin/master && npm install && npm run build",
    "test": "react-scripts test",
    "clear-modules": "rm -r -f ./node_modules && rm -f ./package-lock.json",
    "install:clean": "npm run clear-modules && npm install",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "refresh": "npm run clear-modules && npm run update && npm install",
    "update": "ncu -u -a"
  },
  "dependencies": {
    "@material-ui/core": "3.6.0",
    "@material-ui/icons": "3.0.1",
    "ajv": "^6.5.5",
    "axios": "^0.18.0",
    "chartist": "0.11.0",
    "classnames": "2.2.6",
    "electron-is-dev": "^1.0.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "npm-run-all": "4.1.5",
    "perfect-scrollbar": "1.4.0",
    "react": "16.6.3",
    "react-chartist": "0.13.1",
    "react-dom": "16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.1",
    "react-spinners": "^0.4.7",
    "react-swipeable-views": "0.13.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "optionalDependencies": {
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.15.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.36.2",
    "electron-devtools-installer": "^2.2.4"
  },
  "build": {
    "appId": "com.woutheijnen.trademate",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "productName": "TradeMate",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
}
